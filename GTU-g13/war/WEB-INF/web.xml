<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>User</servlet-name>
		<servlet-class>es.upm.dit.isst.gtu.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>User</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>University</servlet-name>
		<servlet-class>es.upm.dit.isst.gtu.UniversityServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>University</servlet-name>
		<url-pattern>/university</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Bank</servlet-name>
		<servlet-class>es.upm.dit.isst.gtu.BankServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Bank</servlet-name>
		<url-pattern>/bank</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Stamp</servlet-name>
		<servlet-class>es.upm.dit.isst.gtu.StampServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Stamp</servlet-name>
		<url-pattern>/stamp</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
